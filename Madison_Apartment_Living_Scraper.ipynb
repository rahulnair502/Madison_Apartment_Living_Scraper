    "def round_to_nearest_100(number):\n",
    "    number = int(number)\n",
    "  \n",
    "    if number < 500:\n",
    "        return 500\n",
    "    elif number > 5000:\n",
    "        return 5000\n",
    "    else:\n",
    "        return str(round(number / 100) * 100)"
   ]
